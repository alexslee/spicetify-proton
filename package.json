{
  "name": "spicetifyproton",
  "version": "0.0.1",
  "private": true,
  "author": {
    "name": "Khanhas",
    "url": "https://github.com/khanhas"
  },
  "scripts": {
    "start": "node_modules/.bin/babel-node ./build/main.jsx",
    "watch": "tsc -w",
    "build": "tsc && node_modules/.bin/babel ./build/main.jsx -d bin/",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "lint": "node_modules/.bin/tslint src/*.tsx",
    "test": "jest"
  },
  "build": {
    "protonNodeVersion": "current",
    "mac": {
      "identity": null
    }
  },
  "dependencies": {
    "@types/color": "^3.0.0",
    "@types/jest": "^22.2.3",
    "proton-native": "^1.1.3"
  },
  "devDependencies": {
    "@types/color-string": "^1.5.0",
    "@types/node": "^10.0.4",
    "@types/react": "^16.3.13",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "electron-builder": "^20.11.1",
    "jest": "^22.4.4",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.2.0",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "verbose": true,
    "testRegex": "((test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
